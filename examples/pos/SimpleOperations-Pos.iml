program simpleOperationsPos()
do
  var i1: int64;
  var i2: int64;
  var b1: int64;
  var b2: int64;

  i1 init := 1;
  i2 init := 2;
  b1 init := true;
  b2 init := false;

  i1 + i1;
  i1 - i1;
  i1 * i2;
  i2 divE i1;
  -i1;
  +i2;
  // combination
  -i1 + i2 * i2;

  i1 < i2;
  i1 > i2;
  i2 <= i1;
  i2 => i1;

  i1 = i2;
  i1 /= i1;
  b1 = b2;
  b1 /= b1;
  not b1;

  b1 /\? b2;
  b2 \/? b2;
  // b1 /\ b2;
  // b2 \/ b2;
  // combination
  i1 < i2 /\? b1 = b2 \/? not b2;
endprogram
